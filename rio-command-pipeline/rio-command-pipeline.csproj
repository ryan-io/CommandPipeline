<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <RootNamespace>rio_command_pipeline</RootNamespace>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Title>RyanIO - Command Pipeline</Title>
        <Description>An asynchronous API for creating a command pipeline that represents a unit of work. </Description>
        <Copyright>RyanIO</Copyright>
        <PackageProjectUrl>https://github.com/ryan-io/CommandPipeline</PackageProjectUrl>
        <RepositoryUrl>https://github.com/ryan-io/CommandPipeline</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
        <AssemblyVersion>1.0.1</AssemblyVersion>
        <FileVersion>1.0.1</FileVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.Logging" Version="7.0.0" />
    </ItemGroup>

</Project>
